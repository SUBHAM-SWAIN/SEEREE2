<table border="1" style="width: 100%; text-align: center">
  <tr>
    <th>si no</th>
    <th>Name</th>
    <th>Email</th>
    <th>Phone</th>
    <th>Age</th>
    <th>Gender</th>
    <th>Action</th>
  </tr>
  <% users.forEach((user,index) => { %>
  <tr>
    <td><%= index+1 %></td>
    <td><%= user.name %></td>
    <td><%= user.email %></td>
    <td><%= user.phone %></td>
    <td><%= user.age %></td>
    <td><%= user.gender %></td>
    <td>
      <a href="/api/editUser/<%= user._id %>">Edit</a>|<a
        href="/api/deleteUSer/<%= user._id %>"
        >Delete</a
      >
    </td>
    <% }) %>
  </tr>
</table>
